    "angular-messages": "^1.5.5",
    "angular-mocks": "^1.5.5",
    "angular-route": "^1.5.5",
    "angular-sanitize": "^1.5.5",
    "browserify": "^13.0.1",
    "chalk": "^1.1.3",
    "concat-stream": "^1.5.1",
    "crypto": "0.0.3",
    "csserror": "^2.0.2",
    "express": "^4.13.3",
    "gaze": "^1.1.0",
    "hammerjs": "^2.0.8",
    "inline-source-map-comment": "^1.0.5",
    "jade": "^1.11.0",
