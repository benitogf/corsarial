{
  "name": "Corsarial",
  "version": "1.0.1",
  "description": "Cordova app sample with angular material.",
  "license": "MIT",
  "repository": "benitogf/corsarial",
  "main": "index.html",
  "window": {
    "title": "Corsarial",
    "icon": "img/logo.png",
    "min_width": 960,
    "min_height": 700
  },
  "scripts": {
    "specs": "mocha",
    "buildjs": "browserify -t stringify -t require-globify ./src/index.js | uglifyjs > ./www/js/index.min.js",
    "buildnw": "cordova build browser --config=nw",
    "build": "cordova build",
    "platform": "cordova platform add",
    "watch": "watchify -t stringify -t require-globify -p [ ./server ] ./src/index.js -o ./www/js/index.bundle.js",
    "test": "./node_modules/.bin/protractor protractor.conf.js",
    "pretest": "./node_modules/.bin/webdriver-manager update",
    "start": "npm run watch",
    "server": "node index.js"
  },
  "author": "",
  "dependencies": {
    "bistre": "^1.0.1",
    "bole": "^3.0.1",
    "compression": "^1.6.2",
    "express": "^4.13.3",
    "xml2js": "^0.4.13"
  },
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-aria": "^1.5.5",
    "angular-cookies": "^1.5.8",
    "angular-material": "^1.0.8",
    "angular-messages": "^1.5.5",
    "angular-mocks": "^1.5.8",
    "angular-route": "^1.5.5",
    "angular-translate": "^2.11.1",
    "angular-translate-storage-cookie": "^2.11.1",
    "angular-translate-storage-local": "^2.11.1",
    "browserify": "^13.0.1",
    "browserify-aes": "^1.0.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "clipboard": "^1.5.12",
    "concat-stream": "^1.5.1",
    "cordova": "^6.3.0",
    "crypto": "0.0.3",
    "csserror": "^2.0.2",
    "d3": "^4.2.0",
    "gaze": "^1.1.0",
    "hammerjs": "^2.0.8",
    "inline-source-map-comment": "^1.0.5",
    "jquery": "^2.1.4",
    "jsdom": "^9.4.2",
    "lodash": "^4.13.1",
    "mocha": "^3.0.1",
    "moment": "^2.14.1",
    "node-sass": "^3.4.2",
    "node-sass-evergreen": "^1.1.2",
    "opn": "^4.0.2",
    "prepend-file": "^1.3.0",
    "protractor": "^4.0.2",
    "pug": "^2.0.0-beta5",
    "replacestream": "^4.0.0",
    "require-globify": "^1.4.1",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.3.7",
    "stringify": "^5.1.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.2.2"
  }
}
