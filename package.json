{
  "name": "corsarial",
  "version": "1.0.0",
  "description": "cordova sample with angular material",
  "scripts": {
    "build": "browserify -t stringify ./src/index.js | uglifyjs > ./www/js/index.min.js",
    "watch": "watchify -t stringify -p [ ./server.js --host localhost --port 9001 ] ./src/index.js -o ./www/js/index.bundle.js",
    "test": "node --harmony test/index.js",
    "start": "node index.js"
  },
