{
  "name": "corsarial",
  "version": "1.0.0",
  "description": "cordova sample with angular material",
  "main": "index.html",
  "chromium-args": " --enable-experimental-web-platform-features",
  "window": {
    "title": "Corsar",
    "position": "center",
    "show": true,
    "frame": true,
    "toolbar": false,
    "icon": "img/logo.png",
    "width": 960,
    "height": 700
  },
  "scripts": {
    "build": "browserify -t stringify ./src/index.js | uglifyjs > ./www/js/index.min.js",
    "watch": "watchify -t stringify ./src/index.js -o ./www/js/index.bundle.js",
    "test": "node --harmony test/index.js",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-aria": "^1.5.5",
    "angular-material": "^1.0.8",
    "angular-messages": "^1.5.5",
    "angular-mocks": "^1.5.5",
    "angular-route": "^1.5.5",
    "angular-sanitize": "^1.5.5",
    "browserify": "^13.0.1",
    "express": "^4.13.3",
    "express-compile-sass": "^3.0.4",
    "hammerjs": "^2.0.8",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "node-sass": "^3.4.2",
    "selenium-webdriver": "^2.53.2",
    "socket.io-client": "^1.3.7",
    "stringify": "^5.1.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.2.2",
    "xml2js": "^0.4.13"
  }
}
