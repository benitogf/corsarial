<?xml version="1.0" encoding="utf-8"?>
<widget id="com.rando.corsarial" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Corsarial</name>
  <server>
    <host>localhost</host>
    <port>9000</port>
    <gapi>gapi</gapi>
    <ganalytics>ganalytics</ganalytics>
  </server>
  <description>A sample Apache Cordova application.</description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">Apache Cordova Team</author>
  <hook type="before_build" src="hooks/build.js" />
  <hook type="before_run" src="hooks/build.js" />
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <preference name="windows-target-version" value="10" />
  <platform name="firefoxos">
    <icon src="www/img/logo.png" width="128" height="128" />
  </platform>
  <platform name="android">
    <allow-intent href="market:*" />
    <icon src="www/img/logo.png" width="128" height="128" density="mdpi" />
  </platform>
  <platform name="windows">
    <icon src="www/img/logo.png" width="128" height="128" />
  </platform>
  <platform name="wp8">
    <icon src="www/img/logo.png" width="128" height="128" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon src="www/img/logo.png" width="128" height="128" />
  </platform>
  <plugin name="cordova-plugin-dialogs" spec="~1.2.1" />
  <plugin name="cordova-plugin-whitelist" spec="~1.2.2" />
</widget>
